{
  "name": "Real-Debrid Manager",
  "short_name": "RD Manager",
  "start_url": ".",
  "display": "standalone",
  "background_color": "#f4f4f4",
  "theme_color": "#007bff",
  "description": "A simple manager for Real-Debrid magnet links and torrents.",
  "icons": [
    {
      "src": "icon-192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "icon-512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/x-bittorrent": [".torrent"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+magnet",
      "url": "/?url=%s"
    }
  ]
}